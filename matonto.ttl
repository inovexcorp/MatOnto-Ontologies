###  http://matonto.org/ontologies/matonto#BandGap

:BandGap rdf:type owl:Class ;
         
         rdfs:label "band gap" ;
         
         rdfs:subClassOf dl:MeasuredProperty ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty dl:hasUnit ;
                           owl:allValuesFrom :BandGapUnit
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty dl:hasUnit ;
                           owl:someValuesFrom :BandGapUnit
                         ] ;
         
         rdfs:comment "The energy difference between the top of the valence band and the botto of the conduction band in insulators and semiconductors" .



###  http://matonto.org/ontologies/matonto#BandGapUnit

:BandGapUnit rdf:type owl:Class ;
             
             rdfs:label "band gap unit" ;
             
             rdfs:subClassOf dl:Unit ;
             
             rdfs:comment "A standard unit for band gap" .



###  http://matonto.org/ontologies/matonto#Ceramics

:Ceramics rdf:type owl:Class ;
          
          rdfs:label "ceramics" ;
          
          rdfs:subClassOf :MaterialCategory .



