#
#################################################################


###  http://matonto.org/ontologies/2014/03/phaseCharacterization#hasElement

:hasElement rdf:type owl:ObjectProperty ;
            
            rdfs:label "Has Element"@en ;
            
            rdfs:comment "A structure has one or many chemical elements"@en ;
            
            rdfs:domain :Structure ;
            
            rdfs:range dl:Element .



###  http://matonto.org/ontologies/2014/03/phaseCharacterization#hasPhaseEquilibrium

:hasPhaseEquilibrium rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "Has Phase Equilibrium"@en ;
                     
                     rdfs:comment "A system may have one or many phase equilibriums"@en ;
                   
                     ## What else can have a Phase Equilibrium?
					 
                     rdfs:domain :Alloy ;
				
                     rdfs:domain dl:Compound ;
                     
                     rdfs:range :PhaseEquilibrium .



###  http://matonto.org/ontologies/2014/03/phaseCharacterization#hasPressure

:hasPressure rdf:type owl:ObjectProperty ;
             
             rdfs:label "Has Pressure"@en ;
             
             rdfs:comment "A phase equilibrium occurs at a specific pressure"@en ;
             
             rdfs:domain :PhaseEquilibrium ;
             
             rdfs:range dl:Pressure .



###  http://matonto.org/ontologies/2014/03/phaseCharacterization#hasStructure

:hasStructure rdf:type owl:ObjectProperty ;
              
              rdfs:label "Has Structure"@en ;
              
              rdfs:comment "A phase equilibrium has at least two chemical structures"@en ;
              
              rdfs:range :Structure .



###  http://matonto.org/ontologies/2014/03/phaseCharacterization#hasTemperature

:hasTemperature rdf:type owl:FunctionalProperty ,
                         owl:ObjectProperty ;
                
                rdfs:label "Has Temperature"@en ;
                
                rdfs:comment "A phase equilibrium has a specific temperature"@en ;
                
                rdfs:domain :PhaseEquilibrium ;
                
                rdfs:range dl:Temperature .





#################################################################
